			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "‚ûï Add cough",
				"target_preset": "new",
				"appearance": "",
				"actions": [
					{
						"type": "add",
						"target": "add(entry)",
						"filter_junction": "and",
						"set_pairs": [],
						"filter_pairs": []
					}
				]
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"group_by": "month",
				"type": "line",
				"orientation": "horizontal",
