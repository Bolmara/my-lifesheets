				"appearance": ""
			},
			"name": "timesince"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "number",
				"number_style": "number",
				"appearance": "",
				"prefix": "Intensity - ",
				"number_edit": "slider",
				"max": 10
			},
			"name": "intensity"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {
				"appearance": "",
				"min_width": 4,
				"min_width_unit": "%"
			}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"prefix": "Frequency (roughly) - ",
				"max": 20,
				"number_edit": "slider",
				"allow_multiple": "",
				"suffix": "",
