	"root": [
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "'Time since last entry - ' & duration($now - last(date_time))",
				"text_color": "#de3e2d",
				"formatting": [
					"b"
				],
				"appearance": "line"
			},
			"name": "timesince2"
		},
		{
			"type": "spacer",
			"scope": "root",
			"settings": {
				"appearance": "line",
				"min_width": 30
			}
		},
