			},
			"name": "frequency"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {
				"appearance": "stretch",
				"min_width_unit": "%",
				"min_width": 4
			}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"prefix": "Gave half pill ? ",
				"type": "toggle",
				"toggle_type": "multiple",
				"options": "‚úñ, üíä,  üíäüíä",
				"allow_multiple": "",
				"initial_items": "0"
			},
			"name": "halfpill"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {
				"appearance": "line"
			}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
